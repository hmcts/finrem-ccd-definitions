{
  "name": "finrem-ccd-definitions",
  "version": "1.0.0",
  "description": "Utility to help creating CCD config definitions",
  "main": "index.js",
  "repository": "git@github.com:hmcts/fr-ccd-definitions.git",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=11.10.1"
  },
  "config": {
    "local": {
      "cosUrl": "http://host.docker.internal:4012",
      "ccdUrl": "http://localhost:4452"
    },
    "demo": {
      "cosUrl": "http://finrem-cos-demo.service.core-compute-demo.internal",
      "ccdUrl": "http://ccd-data-store-api-demo.service.core-compute-demo.internal",
      "aacUrl": "http://aac-manage-case-assignment-demo.service.core-compute-demo.internal"
    },
    "aat": {
      "cosUrl": "http://finrem-cos-aat.service.core-compute-aat.internal",
      "ccdUrl": "http://ccd-data-store-api-aat.service.core-compute-aat.internal",
      "aacUrl": "http://aac-manage-case-assignment-aat.service.core-compute-aat.internal"
    },
    "preview": {
      "cosUrl": "http://finrem-cos-aat.service.core-compute-aat.internal",
      "ccdUrl": "http://ccd-data-store-api-aat.service.core-compute-aat.internal",
      "aacUrl": "http://aac-manage-case-assignment-aat.service.core-compute-aat.internal"
    },
    "ithc": {
      "cosUrl": "http://finrem-cos-ithc.service.core-compute-ithc.internal",
      "ccdUrl": "http://ccd-data-store-api-ithc.service.core-compute-ithc.internal",
      "aacUrl": "http://aac-manage-case-assignment-ithc.service.core-compute-ithc.internal"
    },
    "perftest": {
      "cosUrl": "http://finrem-cos-perftest.service.core-compute-perftest.internal",
      "ccdUrl": "http://ccd-data-store-api-perftest.service.core-compute-perftest.internal",
      "aacUrl": "http://aac-manage-case-assignment-perftest.service.core-compute-perftest.internal"
    },
    "prod": {
      "cosUrl": "http://finrem-cos-prod.service.core-compute-prod.internal",
      "ccdUrl": "http://ccd-data-store-api-prod.service.core-compute-prod.internal",
      "aacUrl": "http://aac-manage-case-assignment-prod.service.core-compute-prod.internal"
    }
  },
  "scripts": {
    "reset-ccd-submodule": "git submodule update --init --recursive && git submodule foreach --recursive git pull origin master && yarn --cwd ccd-definition-processor install",
    "setup": "git submodule update --init --recursive && yarn --cwd ccd-definition-processor install",
    "lint": "eslint --fix .",
    "test": "NODE_PATH=. mocha test/unit/**/*.js",
    "test:all": "yarn lint; yarn test; yarn test:a11y; yarn test:functional; yarn audit",
    "test:coverage": "echo test:coverage",
    "test:a11y": "echo test:a11y",
    "test:smoke": "echo smoke",
    "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --steps -c test/functional/codecept.conf.js --grep @pipeline --reporter mochawesome",
    "test:fullfunctional": "NODE_TLS_REJECT_UNAUTHORIZED=0 TESTS_FOR_ACCESSIBILITY='true' codeceptjs run-multiple parallel --steps -c test/functional/codecept.conf.js --grep @nightly --reporter mochawesome",
    "test-crossbrowser-e2e": "TESTS_FOR_CROSS_BROWSER='true' codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} --plugins retryFailedStep --steps -c test/functional/saucelabs.conf.js --grep @crossbrowser --reporter mochawesome",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh",
    "start": "node index.js",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "generate-json-consented": "yarn --cwd ccd-definition-processor xlsx2json -D ../definitions/consented/json -i ../definitions/consented/xlsx/ccd-config-base-consented.xlsx && pretty-quick --pattern 'definitions/**/json/**.json'",
    "generate-excel-consented": "yarn run copy-common-components-consented && CCD_DEF_CASE_TYPE_ID=FinancialRemedyMVP2 yarn --cwd ccd-definition-processor json2xlsx -D ../definitions/consented/json -e ${EXCLUDE} -o ../definitions/consented/xlsx/ccd-config-${FR_ENV:-base}-consented-${GIT_COMMIT:-base}.xlsx",
    "generate-excel-local-consented": "FR_ENV=local CCD_DEF_COS_URL=http://localhost:9000 CCD_DEF_CCD_URL=$npm_package_config_local_ccdUrl EXCLUDE='*-prod.json' yarn run generate-excel-consented",
    "generate-excel-preview-consented": "FR_ENV=preview  CCD_DEF_COS_URL=$COS_API_URL CCD_DEF_CCD_URL=$CCD_DATA_API_URL CCD_DEF_AAC_URL=$AAC_API_URL EXCLUDE='*-prod.json' yarn run generate-excel-consented",
    "generate-excel-aat-consented": "FR_ENV=aat CCD_DEF_COS_URL=$npm_package_config_aat_cosUrl CCD_DEF_CCD_URL=$npm_package_config_aat_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_aat_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-consented",
    "generate-excel-demo-consented": "FR_ENV=demo CCD_DEF_COS_URL=$npm_package_config_demo_cosUrl CCD_DEF_CCD_URL=$npm_package_config_demo_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_demo_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-consented",
    "generate-excel-demo-prod-like-consented": "FR_ENV=demo-prod-like CCD_DEF_COS_URL=$npm_package_config_demo_cosUrl CCD_DEF_CCD_URL=$npm_package_config_demo_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_demo_aacUrl EXCLUDE='*-nonprod.json' yarn run generate-excel-consented",
    "generate-excel-ithc-consented": "FR_ENV=ithc CCD_DEF_COS_URL=$npm_package_config_ithc_cosUrl CCD_DEF_CCD_URL=$npm_package_config_ithc_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_ithc_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-consented",
    "generate-excel-perftest-consented": "FR_ENV=perftest CCD_DEF_COS_URL=$npm_package_config_perftest_cosUrl CCD_DEF_CCD_URL=$npm_package_config_perftest_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_perftest_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-consented",
    "generate-excel-prod-consented": "FR_ENV=prod CCD_DEF_COS_URL=$npm_package_config_prod_cosUrl CCD_DEF_CCD_URL=$npm_package_config_prod_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_prod_aacUrl EXCLUDE='*-nonprod.json' yarn run generate-excel-consented",
    "generate-excel-all-consented": "yarn generate-excel-consented && yarn generate-excel-local-consented && yarn generate-excel-demo-consented && yarn generate-excel-aat-consented && yarn generate-excel-ithc-consented && yarn generate-excel-demo-prod-like-consented && yarn generate-excel-perftest-consented && yarn generate-excel-prod-consented",
    "generate-json-contested": "yarn --cwd ccd-definition-processor xlsx2json -D ../definitions/contested/json -i ../definitions/contested/xlsx/ccd-config-base-contested.xlsx && pretty-quick --pattern 'definitions/**/json/**.json'",
    "generate-excel-contested": "yarn run copy-common-components-contested && CCD_DEF_CASE_TYPE_ID=FinancialRemedyContested yarn --cwd ccd-definition-processor json2xlsx -D ../definitions/contested/json -e ${EXCLUDE} -o ../definitions/contested/xlsx/ccd-config-${FR_ENV:-base}-contested-${GIT_COMMIT:-base}.xlsx",
    "generate-excel-local-contested": "FR_ENV=local CCD_DEF_COS_URL=http://localhost:9000 CCD_DEF_CCD_URL=$npm_package_config_local_ccdUrl EXCLUDE='*-prod.json' yarn run generate-excel-contested",
    "generate-excel-preview-contested": "FR_ENV=preview CCD_DEF_COS_URL=$COS_API_URL CCD_DEF_CCD_URL=$CCD_DATA_API_URL CCD_DEF_AAC_URL=$AAC_API_URL EXCLUDE='*-prod.json' yarn run generate-excel-contested",
    "generate-excel-aat-contested": "FR_ENV=aat CCD_DEF_COS_URL=$npm_package_config_aat_cosUrl CCD_DEF_CCD_URL=$npm_package_config_aat_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_aat_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-contested",
    "generate-excel-demo-contested": "FR_ENV=demo CCD_DEF_COS_URL=$npm_package_config_demo_cosUrl CCD_DEF_CCD_URL=$npm_package_config_demo_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_demo_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-contested",
    "generate-excel-demo-prod-like-contested": "FR_ENV=demo-prod-like CCD_DEF_COS_URL=$npm_package_config_demo_cosUrl CCD_DEF_CCD_URL=$npm_package_config_demo_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_demo_aacUrl EXCLUDE='*-nonprod.json' yarn run generate-excel-contested",
    "generate-excel-ithc-contested": "FR_ENV=ithc CCD_DEF_COS_URL=$npm_package_config_ithc_cosUrl CCD_DEF_CCD_URL=$npm_package_config_ithc_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_ithc_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-contested",
    "generate-excel-perftest-contested": "FR_ENV=perftest CCD_DEF_COS_URL=$npm_package_config_perftest_cosUrl CCD_DEF_CCD_URL=$npm_package_config_perftest_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_perftest_aacUrl EXCLUDE='*-prod.json' yarn run generate-excel-contested",
    "generate-excel-prod-contested": "FR_ENV=prod CCD_DEF_COS_URL=$npm_package_config_prod_cosUrl CCD_DEF_CCD_URL=$npm_package_config_prod_ccdUrl CCD_DEF_AAC_URL=$npm_package_config_prod_aacUrl EXCLUDE='*-nonprod.json' yarn run generate-excel-contested",
    "generate-excel-all-contested": "yarn generate-excel-contested && yarn generate-excel-local-contested && yarn generate-excel-demo-contested && yarn generate-excel-aat-contested && yarn generate-excel-ithc-contested && yarn generate-excel-demo-prod-like-contested && yarn generate-excel-perftest-contested && yarn generate-excel-prod-contested",
    "copy-common-components-consented": "cp -R definitions/common/json/* definitions/consented/json",
    "copy-common-components-contested": "cp -R definitions/common/json/* definitions/contested/json",
    "copy-local-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-local-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-local-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "copy-preview-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-preview-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-preview-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "copy-aat-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-aat-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-aat-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "copy-demo-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-demo-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-demo-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "copy-ithc-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-ithc-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-ithc-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "copy-perftest-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-perftest-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-perftest-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "copy-prod-xlxs-to-resources": "cp -R definitions/contested/xlsx/ccd-config-prod-contested-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition && cp -R definitions/consented/xlsx/ccd-config-prod-consented-${GIT_COMMIT:-base}.xlsx src/test/resources/ccd_definition",
    "generate-excel-local-all": "yarn generate-excel-local-consented && yarn generate-excel-local-contested",
    "highLevelDataSetup": "echo 'bash ./gradlew highLevelDataSetup --args=$1 && sh bin/after-data-setup-step.sh' > ./temp.sh && sh ./temp.sh"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@hmcts/nodejs-healthcheck": "^1.8.0",
    "@hmcts/properties-volume": "0.0.13",
    "codeceptjs": "^2.6.11",
    "config": "^3.3.3",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@hmcts/eslint-config": "^1.4.0",
    "@shinnn/eslint-config-node": "^6.0.2",
    "puppeteer": "^7.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha": "^5.3.0",
    "html_codesniffer": "^2.5.1",
    "husky": "^2.2.0",
    "loadsh": "^0.0.4",
    "mocha": "^6.1.4",
    "mochawesome": "^6.2.1",
    "otp": "^0.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sonar-scanner": "^3.1.0",
    "webdriverio": "^7.0.7"
  },
  "resolutions": {
    "bl": "4.0.3",
    "eslint-utils": "^1.4.1",
    "https-proxy-agent": "4.0.0",
    "lodash": "4.17.21",
    "ini": "1.3.6",
    "axios": "0.21.2",
    "ws": "^7.4.6",
    "hosted-git-info": "^2.8.9",
    "normalize-url": "^4.5.1",
    "path-parse": "^1.0.7",
    "moment": "^2.29.4",
    "ansi-regex": "^5.0.1",
    "json-schema": "^0.4.0",
    "follow-redirects": "^1.14.8",
    "node-fetch": "^2.6.7",
    "validator": "^13.7.0",
    "minimist": "^1.2.6",
    "async": "^3.2.2",
    "got": "^11.8.5",
    "file-type": "16.5.4",
    "minimatch": "^3.0.5",
    "qs": ">=6.5.3",
    "json5": ">=2.2.2",
    "flat": ">=5.0.1",
    "cookiejar": ">=2.1.4"
  }
}
